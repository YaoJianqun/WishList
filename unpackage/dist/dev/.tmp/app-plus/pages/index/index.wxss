.scroll-Y .task-item {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
        -ms-flex-align: center;
            align-items: center;
    position: relative;
    padding: 0 15px 0 25px;
    border-radius: 10px;
    height: 90px;
    margin: 0 15px 15px;
    background-color: #fff;
}
.scroll-Y .task-item .iconfont {
      width: 40px;
      height: 90px;
      line-height: 90px;
      font-size: 40px;
}
.scroll-Y .task-item .task-info {
      height: 65px;
      margin-left: 30px;
}
.scroll-Y .task-item .task-info .title {
        line-height: 35px;
        font-weight: bold;
        font-size: 20px;
}
.scroll-Y .task-item .task-info .info {
        line-height: 25px;
        font-size: 16px;
        color: #9B9B9B;
}
.progress {
  height: 0px;
  width: 180px;
  border-bottom: 6px solid #13CDBB;
  border-bottom-left-radius: 6px;
  position: absolute;
  bottom: 0;
  left: 0;
}
.footer-wrapper {
  background-color: #EEE;
  height: 51px;
  width: 100%;
  padding-top: 15px;
  padding-bottom: 34px;
  position: fixed;
  bottom: 0;
}
.footer-wrapper .dock-bar {
    height: 51px;
    margin: 0 15px;
    border-radius: 10px;
    background-color: #fff;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
}
.footer-wrapper .dock-bar .dock-item {
      width: 25%;
      text-align: center;
      line-height: 51px;
}
.footer-wrapper .dock-bar .dock-item .icon.iconfont {
        font-size: 28px;
        color: #A2A2A2;
}
.footer-wrapper .dock-bar .dock-item .icon.iconfont.checked {
        color: #101010;
}

